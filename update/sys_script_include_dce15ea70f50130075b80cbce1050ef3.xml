        <script><![CDATA[var SarUtil = Class.create();
SarUtil.getUserInitials = function(pUserID) {
	if(pUserID) {
		var _gr = new GlideRecord('sys_user');
		_gr.get(pUserID);
		return (_gr.getRowCount()) ? (_gr.first_name.substr(0, 1) + _gr.last_name.substr(0, 1)).toUpperCase() : false;
	}
	return false;
};
